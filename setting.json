{
  "files.eol": "\n",
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontSize": 14,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "vscode_custom_css.imports": [
    "file:///C:/custom.css"
  ],
  "editor.cursorWidth": 5,
  "vscode_custom_css.policy": true,
  "editor.tokenColorCustomizations": {
    "[Dracula]": {
      "textMateRules": [
        {
          "scope": [
            "entity.other.attribute-name"
          ],
          "settings": {
            "foreground": "#00FB8E",
            "fontStyle": ""
          }
        },
        {
          "scope": [
            "entity.name.function"
          ],
          "settings": {
            "foreground": "#00FB8E",
          }
        },
        {
          "scope": [
            "string",
            "punctuation.definition.string.begin",
            "punctuation.definition.string.end"
          ],
          "settings": {
            "foreground": "#fde181",
          }
        },
        {
          "scope": [
            "entity.name.tag"
          ],
          "settings": {
            "foreground": "#ff6d03",
          }
        },
        {
          "scope": "entity.other.attribute-name.class.css",
          "settings": {
            "foreground": "#ff6d03",
          }
        },
      ]
    }
  },
  "indentRainbow.colors": [
    "rgba(3,4,94,0.125)",
    "rgba(2,62,138,0.125)",
    "rgba(0,119,182,0.125)",
    "rgba(0,150,199,0.125)",
    "rgba(0,180,216,0.125)",
    "rgba(72,202,228,0.125)",
    "rgba(144,224,239,0.125)",
    "rgba(0, 100, 102, 0.125)",
    "rgba(6, 90, 96, 0.125)",
    "rgba(11, 82, 91, 0.125)",
    "rgba(20, 69, 82, 0.125)",
    "rgba(27, 58, 75, 0.125)",
    "rgba(33, 47, 69, 0.125)",
    "rgba(39, 38, 64, 0.125)",
    "rgba(49, 34, 68, 0.125)",
    "rgba(62, 31, 71, 0.125)",
    "rgba(77, 25, 77, 0.125)",
  ],
  // "indentRainbow.colors": [
  //   "rgba(0, 100, 102, 0.25)",
  //   "rgba(6, 90, 96, 0.25)",
  //   "rgba(11, 82, 91, 0.25)",
  //   "rgba(20, 69, 82, 0.25)",
  //   "rgba(27, 58, 75, 0.25)",
  //   "rgba(33, 47, 69, 0.25)",
  //   "rgba(39, 38, 64, 0.25)",
  //   "rgba(49, 34, 68, 0.25)",
  //   "rgba(62, 31, 71, 0.25)",
  //   "rgba(77, 25, 77, 0.25)",
  // ],
  "editor.tabCompletion": "on",
  "explorer.confirmDelete": false,
  "editor.wordWrap": "on",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "turboConsoleLog.quote": "'",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.wordWrapColumn": 35,
  "editor.letterSpacing": 1,
  "editor.lineHeight": 36,
  "editor.fontFamily": "'JetBrains Mono', Consolas, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "'JetBrains Mono', Consolas, 'Courier New', monospace",
  "editor.codeLensFontFamily": "'JetBrains Mono'",
  "editor.inlayHints.fontFamily": "'JetBrains Mono'",
  "scm.inputFontFamily": "'JetBrains Mono'",
  "debug.console.fontFamily": "'JetBrains Mono'",
  "editor.fontLigatures": true,
  // "editor.fontFamily": "SF Mono, Source Code Pro, Input Mono, Inconsolata, Arial, Helvetica, sans-serif",
  // "editor.fontFamily": "Helvetica, SF Mono, Source Code Pro, Input Mono, Inconsolata, Arial, sans-serif",
  // "editor.fontFamily": "Consolas, 'Courier New', monospace",
  "editor.tabSize": 2,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  // "bracket-pair-colorizer-2.depreciation-notice": false,
  // "bracketPairColorizer.depreciation-notice": false,
  "eslint.format.enable": true,
  "workbench.editor.enablePreview": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.donotVerifyTags": true,
  "workbench.colorTheme": "Evondev Dracula Night Contrast",
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.minimap.showSlider": "always",
  "editor.stickyScroll.enabled": true,
  "window.openWithoutArgumentsInNewWindow": "off",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.accessibilitySupport": "off",
  "cSpell.userWords": [
    "Actifs",
    "Flatpickr"
  ],
  "eslint.enable": true,
  "workbench.editor.wrapTabs": true,
  "aws.suppressPrompts": {
    "codeWhispererConnectionExpired": true
  },
  "vscode-edge-devtools.webhint": false,
  "git.openRepositoryInParentFolders": "always",
  "debug.console.wordWrap": false,
  "git.enableSmartCommit": true,
  "window.zoomLevel": 1,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.endOfLine": "cr",
}